"agent": {
  "metrics_collection_interval": 60,
  "run_as_user": "root"
},
"metrics": {
  "metrics_collected": {
          "collectd": {
                  "metrics_aggregation_interval": 60
          },
          "disk": {
                  "measurement": [
                          "used_percent"
                  ],
                  "metrics_collection_interval": 60,
                  "resources": [
                          "*"
                  ]
          },
          "mem": {
                  "measurement": [
                          "mem_used_percent"
                  ],
                  "metrics_collection_interval": 60
          },
          "statsd": {
                  "metrics_aggregation_interval": 60,
                  "metrics_collection_interval": 60,
                  "service_address": ":8125"
          }
  }
}